<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DarkTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="backgroudcolor">@style/page_background_dark</item>
        <item name="styleToolbar">@style/tool_bar_dark</item>
        <item name="appbarTheme">@style/appbar_style_dark</item>
    </style>

    <style name="LightTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="backgroudcolor">@style/page_background_light</item>
        <item name="styleToolbar">@style/tool_bar_light</item>
        <item name="appbarTheme">@style/appbar_style_light</item>

    </style>
</resources>